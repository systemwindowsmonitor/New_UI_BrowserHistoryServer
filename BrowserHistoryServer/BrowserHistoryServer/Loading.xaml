<Window x:Class="BrowserHistoryServer.Loading"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BrowserHistoryServer"
        mc:Ignorable="d"
        Title="Loading"  Background="#0000ffff" Height="600" Width="800" WindowStyle="None" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">

    <Grid x:Name="GridMain">
        <MediaElement x:Name="Media" Visibility="Visible" UnloadedBehavior="Manual" MediaEnded ="Media_MediaEnded" Stretch="Fill"></MediaElement>
    </Grid>
</Window>
